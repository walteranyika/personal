<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shadiih.firstaid1"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/learnaid"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FirstAidActivity"
            android:label="@string/title_activity_first_aid" >
        </activity>
        <activity
            android:name=".ChokingActivity"
            android:label="@string/title_activity_choking"
            android:parentActivityName=".FirstAidActivity" >
        </activity>
        <activity
            android:name=".DisclaimerActivity"
            android:label="@string/title_activity_disclaimer"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".AsthmaAttackActivity"
            android:label="@string/title_activity_asthma_attack"
            android:parentActivityName=".FirstAidActivity" >
        </activity>
        <activity
            android:name=".BitesAndStingsActivity"
            android:label="@string/title_activity_bites_and_stings"
            android:parentActivityName=".FirstAidActivity" >
        </activity>
        <activity
            android:name=".BleedingActivity"
            android:label="@string/title_activity_bleeding"
            android:parentActivityName=".FirstAidActivity" >
        </activity>
        <activity
            android:name=".BrokenBoneActivity"
            android:label="@string/title_activity_broken_bone"
            android:parentActivityName=".FirstAidActivity" >
        </activity>
        <activity
            android:name=".BurnsActivity"
            android:label="@string/title_activity_burns"
            android:parentActivityName=".FirstAidActivity" >
        </activity>
        <activity
            android:name=".DiabetesActivity"
            android:label="@string/title_activity_diabetes"
            android:parentActivityName=".FirstAidActivity" >
        </activity>
        <activity
            android:name=".DistressedActivity"
            android:label="@string/title_activity_distressed"
            android:parentActivityName=".FirstAidActivity" >
        </activity>
        <activity
            android:name=".HeadInjuryActivity"
            android:label="@string/title_activity_head_injury"
            android:parentActivityName=".FirstAidActivity" >
        </activity>
        <activity
            android:name=".HeartAttackActivity"
            android:label="@string/title_activity_heart_attack"
            android:parentActivityName=".FirstAidActivity" >
        </activity>
        <activity
            android:name=".NoseBleedActivity"
            android:label="@string/title_activity_nose_bleed"
            android:parentActivityName=".FirstAidActivity" >
        </activity>
        <activity
            android:name=".PoisonActivity"
            android:label="@string/title_activity_poison"
            android:parentActivityName=".FirstAidActivity" >
        </activity>
        <activity
            android:name=".SeizuresEpilepsyActivity"
            android:label="@string/title_activity_seizures_epilepsy"
            android:parentActivityName=".FirstAidActivity" >
        </activity>
        <activity
            android:name=".StrokeActivity"
            android:label="@string/title_activity_stroke"
            android:parentActivityName=".FirstAidActivity" >
        </activity>
        <activity
            android:name=".UnconsciousAndBreathingActivity"
            android:label="@string/title_activity_unconscious_and_breathing"
            android:parentActivityName=".FirstAidActivity" >
        </activity>
        <activity
            android:name=".WoundsActivity"
            android:label="@string/title_activity_wounds"
            android:parentActivityName=".FirstAidActivity" >
        </activity>
    </application>

</manifest>
